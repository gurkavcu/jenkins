container_commands:
  01_delete_init_groovy:
    command: "rm -f /var/jenkins_home/init.groovy.d/init.groovy"
    ignoreErrors: true
  02_delete_ec2_init_groovy:
    command: "rm -f /var/jenkins_home/init.groovy.d/init-ec2.groovy"
    ignoreErrors: true
  03_jenkins_user:
    command: useradd -u 1000 jenkins || echo 'User already exist!'
  04_jenkins_user_groups:
    command: usermod -aG docker jenkins
